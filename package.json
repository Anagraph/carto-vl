  "devDependencies": {
    "eslint": "^4.15.0",
    "sloc": "^0.2.0",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "@mapbox/vector-tile": "^1.3.0",
    "cartocolor": "^4.0.0",
    "earcut": "^2.1.2",
    "jsdoc": "^3.5.5",
    "jsep": "^0.3.2",
    "lru-cache": "^4.1.1",
